    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.0.3",
    "@vitest/ui": "2.0.3",
    "eslint": "8.35.0",
    "npm-run-all": "4.1.5",
    "prisma": "6.0.1",
    "supertest": "7.0.0",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.3"
